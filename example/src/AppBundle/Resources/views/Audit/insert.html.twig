<tr>
  <td class="col-min">
    <span class="label label-success"><i class="fa fa-plus"></i></span>
  </td>
  <td>
    New <strong>{{ log.source.typLabel }}</strong> {{ audit_assoc(log.source) }} was inserted,
    {{ audit_blame(log.blame) }}
  </td>
  <td class="col-min">{{ time_diff(log.loggedAt) }}</td>
  <td class="col-min">
    <a class="btn btn-xs btn-link" href="{{ path('app_audit_diff', {id: log.id}) }}">
      <i class="fa fa-search" title="Show diff"></i>
    </a>
  </td>
</tr>
